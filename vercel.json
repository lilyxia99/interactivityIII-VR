{
  "rewrites": [
    {
      "source": "/intro/:path*",
      "destination": "/intro/index.html"
    },
    {
      "source": "/intro",
      "destination": "/intro/index.html"
    },
    {
      "source": "/((?!intro).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "trailingSlash": false
}
